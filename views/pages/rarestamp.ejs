}

.stamp img {
    max-width: 100%;
    height: auto;
    border-radius: 8px;
}

.stamp p {
    margin-top: 10px;
    font-size: 1.1em;
    color: #555;
}

.back-icon {
    position: absolute;
    top: 30px;      /* Increased space from the top */
    left: 20px;     /* Space from the left */
    font-size: 30px;  /* Size of the icon */
